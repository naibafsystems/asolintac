<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>
		<field name="id" type="text" class="readonly" label="JGLOBAL_FIELD_ID_LABEL" description="JGLOBAL_FIELD_ID_DESC" size="10" default="0" readonly="true" />
        <field name="title" type="text" label="JGLOBAL_TITLE" description="JFIELD_TITLE_DESC" class="inputbox" size="55" required="true" />
        <field name="alias" type="text" label="JFIELD_ALIAS_LABEL" description="JFIELD_ALIAS_DESC" class="inputbox" size="55" />
        <field name="text" type="editor" class="inputbox" label="JOOMDOC_FIELD_LICENSETEXT_LABEL" buttons="false" filter="safehtml" />
        <field name="default" type="checkbox" label="JOOMDOC_FIELD_DEFAULT_LABEL" description="JOOMDOC_FIELD_DEFAULT_DESC" class="inputbox" default="0" value="1" />
        <field name="state" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" filter="intval" size="1" default="1">
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="-2">JTRASHED</option>
        </field>
        <field name="created" type="calendar" label="JOOMDOC_FIELD_CREATED_LABEL" class="readonly" size="22" readonly="true" format="%Y-%m-%d %H:%M:%S" filter="user_utc" />
        <field name="modified" type="calendar" label="JGLOBAL_FIELD_MODIFIED_LABEL" class="readonly" size="22" readonly="true" format="%Y-%m-%d %H:%M:%S" filter="user_utc" />
        <field name="created_by" type="user" label="JOOMDOC_FIELD_CREATED_BY_LABEL" class="readonly" readonly="true" filter="unset" />
        <field name="modified_by" type="user" label="JGLOBAL_FIELD_MODIFIED_BY_LABEL" class="readonly" readonly="true" filter="unset" />
        <field name="checked_out" type="hidden" filter="unset" />
        <field name="checked_out_time" type="hidden" filter="unset" />
    </fieldset>
</form>